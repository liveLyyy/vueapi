<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liyan.vueapi.mapper.RoleMapper">
    <select id="findById" parameterType="Role">
        select * from role where rid=#{rid}
    </select>

    <select id="findAll" resultType="Role" parameterType="Role">
        select rolename from role
    </select>
</mapper>